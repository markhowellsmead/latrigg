	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "var(--image-border-radius)"
				},
				"color": {
					"background": "var(--wp--preset--color--dark-gray)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"top": "var(--wp--preset--spacing--small)"
					}
				}
			},
			"core/code": {
				"border": {
					"bottom": {
						"color": "var(--wp--preset--color--medlight-gray)",
						"style": "solid",
						"width": "1px"
					},
					"left": {
						"color": "var(--wp--preset--color--medlight-gray)",
						"style": "solid",
						"width": "1px"
					},
					"right": {
