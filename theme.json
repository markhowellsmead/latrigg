						"style": "solid",
						"width": "1px"
					},
					"top": {
						"color": "var(--wp--preset--color--medlight-gray)",
						"style": "solid",
						"width": "1px"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--xlight-primary)",
					"text": "#cc5300"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--regular)",
						"left": "var(--wp--preset--spacing--regular)",
						"right": "var(--wp--preset--spacing--regular)",
						"top": "var(--wp--preset--spacing--regular)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--smaller)"
				}
			},
			"core/footnotes": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/media-text": {
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--large)",
						"left": "var(--wp--preset--spacing--large)",
						"right": "var(--wp--preset--spacing--large)",
						"top": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"core/post-date": {
				"color": {
					"text": "var(--wp--preset--color--gray)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--gray)"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontStyle": "normal"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--xlight-gray)",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--white)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--regular)",
						"left": "var(--wp--preset--spacing--regular)",
						"right": "var(--wp--preset--spacing--xxlarge)",
						"top": "var(--wp--preset--spacing--regular)"
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "var(--wp--preset--color--medlight-gray)",
					"width": "1px"
				},
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--large)",
						"top": "var(--wp--preset--spacing--large)"
					}
				},
				"typography": {
					"fontSize": "3em",
					"letterSpacing": "1em"
				},
				"variations": {
					"dots": {
						"color": {
							"text": "currentColor"
						},
						"spacing": {
							"padding": {
								"left": "0",
								"right": "0"
							}
						},
						"typography": {
							"fontFamily": "inherit",
							"fontSize": "2em",
							"letterSpacing": ".5em",
							"textAlign": "center"
						}
					}
				}
			},
			"core/site-logo": {
				"variations": {
					"rounded": {
						"filter": {
							"duotone": "unset"
						}
					}
				}
			},
			"core/site-title": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			},
			"core/table": {
				"border": {
					"color": "var(--wp--preset--color--light-gray)",
					"style": "solid",
					"width": "1px"
				}
			},
			"sht/page-teaser-cover": {
				"color": {
					"gradient": "var(--wp--preset--gradient--bigger-black-bottom-edge)",
					"text": "var(--wp--preset--color--white)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--body-background)",
			"text": "var(--wp--preset--color--body-text)"
		},
